<% if(success_msg != ''){ %>
    <div class="uk-alert-success uk-text-center font-weight-bolder  uk-border-rounded" style="font-size: 20px;" uk-alert>
        <a class="uk-alert-close" uk-close></a>
        <p><%= success_msg %> </p>
    </div>
    <% } %> <% if(error_msg != ''){ %>
    <div class="uk-alert-danger uk-text-center font-weight-bolder  uk-border-rounded" style="font-size: 20px;" uk-alert>
        <a class="uk-alert-close" uk-close></a>
        <p><%= error_msg %> </p>
    </div>
    <% } %> <% if(warning_msg != ''){ %>
    <div class="uk-alert-warning alert-warning uk-text-center font-weight-bolder  uk-border-rounded"
        style="font-size: 20px;" uk-alert>
        <a class="uk-alert-close" uk-close></a>
        <p><%= warning_msg %> </p>
    </div>
    <% } %>